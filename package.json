{
  "name": "ab-photos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "NEXT_PUBLIC_API_URL=https://ab-photo.pages.dev npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "dev:wrangler": "npm run pages:build && npm run pages:dev",
    "test:prod": "NEXT_PUBLIC_API_URL=https://ab-photo.pages.dev npm run pages:build && npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "deploy:preview": "npm run pages:build && wrangler pages deployment create .vercel/output/static --branch=preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "secrets:local": "./get-from-1password.sh local",
    "secrets:cf": "./get-from-1password.sh cloudflare"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "@tryghost/content-api": "^1.11.24",
    "html-entities": "^2.5.2",
    "next": "14.1.0",
    "next-plausible": "^3.12.0",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "react-mailchimp-subscribe": "^2.1.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.11.3",
    "@cloudflare/workers-types": "^4.20240502.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-mailchimp-subscribe": "^2.1.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "^1.11.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5",
    "vercel": "^34.1.11",
    "wrangler": "^3.101.0"
  }
}
